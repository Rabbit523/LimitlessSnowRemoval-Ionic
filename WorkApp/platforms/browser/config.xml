<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ivanov.LimitlessSnowRemoval" version="0.0.7" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Limitless Snow Removal</name>
    <description>An awesome Ionic Framework 3.</description>
    <author email="yuiiwanov523@gmail.com">Yuri Ivanov</author>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <config-file parent="/resources" target="./res/values/strings.xml">
        <string name="fb_app_id">229144901207708</string>
        <string name="fb_app_name">LimitlessSnowRemoval</string>
        <string name="fb_login_protocol_scheme">fb229144901207708</string>
    </config-file>
    <content src="index.html" />
    <allow-navigation href="http://192.168.56.1:8100" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="1000" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="ShowSplashScreen" value="true" />
</widget>
